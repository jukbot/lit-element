{
  "compilerOptions": {
    "target": "es2017",
    "module": "es2015",
    "lib": [
      "es2017",
      "dom"
    ],
    "declaration": false,
    "sourceMap": true,
    "inlineSources": true,
    "outDir": "./lib",
    "baseUrl": ".",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "src/*.ts"
  ],
  "exclude": []
}
